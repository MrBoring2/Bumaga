<Application x:Class="DemExBumagaProgramm.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DemExBumagaProgramm"
             xmlns:converters="clr-namespace:DemExBumagaProgramm.Resources.Converters"
             ShutdownMode="OnLastWindowClose">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/MainTheme.xaml"/>
                <ResourceDictionary>
                    <converters:AgentColorConverter x:Key="agentColorConv"/>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
